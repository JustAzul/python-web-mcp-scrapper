# Example environment variables for the Web Scraper MCP Tool

# Timeout for Playwright navigation and page loading (in seconds)
SCRAPER_TIMEOUT_SECONDS=30

# User agent string for the headless browser
# SCRAPER_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) ..."

# Viewport dimensions for the headless browser page
SCRAPER_VIEWPORT_WIDTH=1920
SCRAPER_VIEWPORT_HEIGHT=1080 